{
	"manifest_version" : 2,
	"name" : "Remembweb",
	"version": "1.0",

	"description": "Save data from Website",

	"icons":{
	"48": "icons/border-48.png"
	},

	"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["remembweb.js"]
	}
	],
	"background": {
	"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icons/border-48.png",
		"default_popup": "popup.html",
		"default_title": "A popup will come here"
	}
}
