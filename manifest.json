{
  "manifest_version": 2,
  "name": "Webmember",
  "version": "0.1",
  "icons":{
      "64": "brain.png"
    },
  "permissions": ["storage"],
  "applications": {
    "gecko": {
      "id": "janin.grandne@gmail.com"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "brain.png",
    "default_popup": "sketch/index.html",
    "default_title": "Webmember"
  }
}
